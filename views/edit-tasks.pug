extends nav-lists
block main-container
  .edit-container
    h2= task.name
    .create-note
      form.create-form__form
        label(for='new-note') New Note:
        textarea(id='new-note' class='create-form__textarea' type='text' name='note' value= note)
        input(class='create-form__save-button' type='button') Save
    .notes
      .notes__note-list
        each note in notes
          .notes__note-list__div= note.note
